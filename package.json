{
  "engine": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "devDependencies": {
    "@types/autoprefixer": "9.5.0",
    "@types/copy-webpack-plugin": "5.0.0",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/jest": "24.0.15",
    "@types/mini-css-extract-plugin": "0.2.1",
    "@types/node": "12.6.2",
    "@types/optimize-css-assets-webpack-plugin": "1.3.4",
    "@types/uglifyjs-webpack-plugin": "1.1.0",
    "@types/webpack-merge": "4.1.5",
    "@types/workbox-webpack-plugin": "4.1.0",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.12.0",
    "autoprefixer": "9.6.1",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "3.0.0",
    "elm": "0.19.0",
    "elm-format": "0.8.1",
    "elm-test": "0.19.0",
    "elm-webpack-loader": "5.0.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "lint-staged": "9.2.0",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "4.12.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "typescript": "3.5.3",
    "uglifyjs-webpack-plugin": "2.1.3",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-merge": "4.2.1",
    "workbox-webpack-plugin": "4.3.1",
    "write-file-webpack-plugin": "4.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy",
      "\\.elm$": "<rootDir>/mocks/elm.ts"
    },
    "roots": [
      "<rootDir>/tests"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "npm run prettify:typescript",
      "git add"
    ],
    "**/*.elm": [
      "npm run prettify:elm",
      "git add"
    ]
  },
  "scripts": {
    "build": "webpack",
    "build:production": "npm run clean && NODE_ENV=production npm run build",
    "clean": "rimraf build",
    "prettify": "npm run prettify:typescript && npm run prettify:elm",
    "prettify:elm": "elm-format --yes .",
    "prettify:typescript": "eslint --fix . --ext .ts",
    "start": "webpack-dev-server --port 3000",
    "test": "jest --coverage && elm-test"
  }
}
